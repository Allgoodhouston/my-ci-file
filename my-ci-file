//here_i_am

pipeline{
    agent any
    
    stages{
        
        stage("stage 1"){
            steps{
                script{
                    echo 'hello from script 1!'
                }
            }
        }
        
        stage("stage 2"){
            steps{
                script{
                    sleep 30
                    echo 'hello from script 2!'
                }
            }
            
        }
        
    }
}

